<div class="container">
  <div class="row">
    <div class="col-4">
      <div class="tinder--cards">
        <% @questions.each do |question|%>
          <div class="tinder--card">
            <%= image_tag question.photourl%>
            <h3>Titulo</h3>
            <p><%= question.description%> </p>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

<div class="tinder--buttons">
 <button id="nope">
      <i class="fa fa-times" style="color: #54436B;"></i>
  </button>
  <button id="love">
      <i class="fa fa-heart"></i>
 </button>
</div>
<form action=<%="/results"%> method="post"> <button type="submit" class="btn btn-lg px-4 me-md-2 start-now-btn">RESULTADO</button>
